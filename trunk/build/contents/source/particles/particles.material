material tracks/DustMat
{
	receive_shadows off
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			alpha_rejection greater 2
			depth_write off

			texture_unit
			{
				texture dust.dds
				tex_address_mode clamp
			}
		}
	}
}

