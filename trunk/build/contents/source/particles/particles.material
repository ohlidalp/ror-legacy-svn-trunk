material tracks/HeatHazeMat
{
	receive_shadows off
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			alpha_rejection greater 2
			depth_write off

			texture_unit
			{
				texture heathaze.png
				tex_address_mode clamp
				wave_xform scroll_x sine 0 0.3 0 0.15
			}
			texture_unit
			{
				colour_op_ex add src_current src_texture
			}
		}
	}
}
