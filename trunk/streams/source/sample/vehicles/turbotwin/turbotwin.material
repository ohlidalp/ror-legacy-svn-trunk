material tracks/turbotwin
{
	technique
	{
		pass
		{
			scene_blend alpha_blend
			alpha_rejection greater 128 
			texture_unit
			{
				texture turbotwin.dds
			}
		}
		pass
		{
			scene_blend add
			depth_write off
			lighting off
			texture_unit
			{
				texture turbotwin_emissive.dds
			}
		}
	}
}

