material tracks/wrecker
{
	technique
	{
		pass
		{
			scene_blend alpha_blend
			alpha_rejection greater 128 
			texture_unit
			{
				texture wrecker.dds
			}
		}
		pass
		{
			scene_blend add
			depth_write off
			lighting off
			texture_unit
			{
				texture wrecker_emissive.dds
			}
		}
	}
}

material tracks/wreckerhelp
{
	technique
	{
		pass
		{

			texture_unit
			{
				texture wreckerhelp.dds
			}
		}
	}
}
